{
    "name": "openLRSng - Configurator",
    "description": "Cool crossplatform Configurator GUI for openLRSng project",
    "version": "0.3",
    "manifest_version": 2,
    "minimum_chrome_version": "23",
    
    "app": {
        "background": {
            "scripts": [ "background.js" ]
        }
    },
    
    "permissions": [
        "https://www.google-analytics.com/",
        "serial",
        "storage",
        "fileSystem"
    ],
    
    "icons": {
        "128": "images/icon_128.png"
    }
}